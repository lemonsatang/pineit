<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.pineit.pineit.mapper.UserMapper">

    <select id="getUserInfo" resultType="com.pineit.pineit.dto.UserDTO">
        SELECT *
            FROM ZUSRLST
            WHERE USRID = #{usrid}
    </select>

</mapper>